{% extends "default.njk" %}

{% block main %}
<form class="main__container -!-container" method="post" novalidate>
{{ errorSummary({
  errorList: errorList(errors)
}) if errors }}

{% block form %}
{% call fieldset({
  legend: {
    classes: "-!-visually-hidden" if minimalui,
    text: heading({ text: title, parent: parent })
  }
}) -%}
  {% block fieldset %}
  {% endblock %}
{% endcall %}
{% endblock %}

{% block buttons %}
{% endblock %}
</form>
{% endblock %}